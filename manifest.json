{
    "manifest_version": 3,
    "name": "YouTube Ad Blocker",
    "version": "1.0",
    "description": "Removes ads from YouTube",
    "permissions": [
      "activeTab"
    ],
    "host_permissions": [
      "*://*.youtube.com/*"
    ],
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["content.js"],
        "run_at": "document_start"
      }
    ]
  }